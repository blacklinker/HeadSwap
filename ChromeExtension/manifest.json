{
    "manifest_version": 3,
    "name": "My Extension for HeadSwap",
    "description": "A simple Chrome extension for my HeadSwap project",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ]
}